<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/logo_lan"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.StudyWithMe"
        tools:targetApi="31">
        <activity
            android:name="comp3350.studywithme.presentation.AddPageActivity"
            android:exported="false" />
        <activity
            android:name="comp3350.studywithme.presentation.ViewNotes"
            android:exported="false" />
        <activity
            android:name="comp3350.studywithme.presentation.ModifyNotesActivity"
            android:exported="false" />
        <activity
            android:name="comp3350.studywithme.presentation.AddNotesActivity"
            android:exported="false"
            android:label="@string/title_activity_add_notes"
            android:theme="@style/Theme.StudyWithMe.NoActionBar" />
        <activity
            android:name="comp3350.studywithme.presentation.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>